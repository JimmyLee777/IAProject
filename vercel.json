{
    "version": 2,
    "builds": [
        {
        "src": "IAproject/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.12"
        }
        }
    ],
    "routes": [
        {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
        },
        {
        "src": "/(.*)",
        "dest": "IAproject/wsgi.py"
        }
    ],
    "build": {
    "env": {
        "STATIC_ROOT": "staticfiles"
    },
    "commands": [
        "pip install -r requirements.txt",
        "python manage.py collectstatic --noinput"
    ]
    }
}